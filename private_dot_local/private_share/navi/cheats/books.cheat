% books

$ html_title: find ~/main/data/books -type f -name "index.html" | sort | while IFS= read -r file; do title=$(cat $file | pup "title text{}"); echo "$title"; done | nl --- --map "cut -f1"

% book, open
@ books

# open a book
html_file=find ~/main/data/books -type f -name "index.html" | sort | sed -n <html_title>p
echo $html_file
